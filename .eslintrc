{
  "extends": "airbnb",
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "comma-dangle": 0,
    "object-curly-spacing": ["error", "never"],
    "no-constant-condition": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-restricted-properties": 0,
  },
  "env": {
    "browser": true
  }
}
